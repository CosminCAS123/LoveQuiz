{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },


  "AllowedHosts": "*",

  "GeneralRules": [
    {
      "Endpoint": "POST:/api/quiz/log-free-session",
      "Period": "1d",
      "Limit": 5
    },
    {
      "Endpoint": "POST:/api/quiz/free-report",
      "Period": "5m",
      "Limit": 10
    }
  ]
 
}
